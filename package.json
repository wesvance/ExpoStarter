{
  "name": "starter",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start --dev-client",
    "web": "expo start --web",
    "build-web:local": "expo build:web",
    "build-ios:local": "eas build --profile local --platform ios --local",
    "build-android:local": "eas build --profile local --platform android --local",
    "test": "jest --watch --passWithNoTests",
    "lint": "eslint ./src",
    "format": "prettier --write .",
    "eject": "expo eject",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --no-manager-cache",
    "deploy-storybook": "storybook-to-ghpages",
    "file": "plop",
    "postinstall": "patch-package",
    "prebuild": "npx expo prebuild --clean",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@expo/vector-icons": "^13.0.0",
    "@logrocket/react-native": "^1.5.0",
    "@notifee/react-native": "^5.6.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/netinfo": "9.3.5",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.4.1",
    "@rnmapbox/maps": "^10.0.0-beta.54",
    "@sentry/react-native": "4.2.2",
    "expo": "^47.0.0-beta.8",
    "expo-application": "~5.0.1",
    "expo-asset": "~8.6.2",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.0",
    "expo-device": "~5.0.0",
    "expo-font": "~11.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-localization": "~14.0.0",
    "expo-splash-screen": "~0.17.3",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.3",
    "i18n-js": "^3.8.0",
    "jest-expo": "^47.0.0-beta",
    "logrocket": "^3.0.1",
    "mapbox-gl": "^2.10.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.4",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-logs": "^4.0.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-redash": "^18.0.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.7",
    "react-query": "^3.39.1",
    "sentry-expo": "~5.0.0",
    "styled-components": "^5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@expo/config": "^7.0.2",
    "@expo/webpack-config": "^0.17.2",
    "@react-native-community/eslint-config": "2.0.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/addon-viewport": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/react-native": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.4.0",
    "@types/mapbox-gl": "^2.6.3",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^17.0.1",
    "@types/seedrandom": "^3.0.2",
    "@types/styled-components": "^5.1.22",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "^5.19.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chromatic": "^6.4.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.7",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": "12.3.3",
    "plop": "^3.0.5",
    "prettier": "2.5.1",
    "prettier-plugin-sort-imports": "^1.6.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "standard-version-expo": "^1.0.3",
    "storybook-addon-designs": "^6.3.1",
    "storybook-addon-styled-component-theme": "^2.0.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|native-base-*)"
    ],
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "resolutions": {
    "styled-components": "^5",
    "@expo/config-plugins": "5.0.0"
  }
}
